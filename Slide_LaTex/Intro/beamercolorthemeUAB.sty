\setbeamercolor{alerted text}{fg=UABdarkgreen!80!UAByellow}
\setbeamercolor*{palette primary}{fg=UABdarkgreen,bg=Gold2}				%Title Color
\setbeamercolor*{palette secondary}{fg=UABdarkgreen,bg=white}
