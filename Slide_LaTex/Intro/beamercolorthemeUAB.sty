\mode<presentation>


\definecolor{darkblue}{rgb}{0,0,0.8}
\definecolor{UABdarkgreen}{rgb}{0.12,0.42,0.32}
\definecolor{UABlightgreen}{rgb}{0.64,0.84,0.36}
\definecolor{UAByellow}{rgb}{0.96,0.76,0}
